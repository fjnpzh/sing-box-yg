- name: vless-fd-yg-$snb
  type: vless
  server: www.web.com
  port: 2087
  uuid: 2bbd9dc8-1a8a-4a2c-80e5-5cfb61636870
  udp: false
  tls: true
  network: ws
  servername: yg.fz.cloudns.ch
